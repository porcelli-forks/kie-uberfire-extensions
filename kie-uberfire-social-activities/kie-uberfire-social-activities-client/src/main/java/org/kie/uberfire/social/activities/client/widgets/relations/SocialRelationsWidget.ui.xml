<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:HTMLPanel>
        <g:HTMLPanel>
            <b:Heading ui:field="title" size="H4"/>
        </g:HTMLPanel>
        <b:Heading size="H3" text="Following ">
            <b:Badge ui:field="followingBadge"/>
        </b:Heading>
        <b:ListGroup ui:field="allfollowing"/>
        <b:Heading size="H3" text="Followers">
            <b:Badge ui:field="followersBadge"/>
        </b:Heading>
        <b:ListGroup ui:field="allfollowers"/>
        <b:Heading size="H3" text="All users">
            <b:Badge ui:field="allusersBadge"/>
        </b:Heading>
        <b:ListGroup ui:field="allusers"/>
    </g:HTMLPanel>
</ui:UiBinder>